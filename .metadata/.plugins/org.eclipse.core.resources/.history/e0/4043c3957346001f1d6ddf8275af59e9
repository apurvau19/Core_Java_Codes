		Employee e2= new Employee();
		Employee e3= new Employee();
		
		e1.id=1;
		e1.name="Sourabh";
		e1.salary=50000;
		e1.age=27;
		
		e2.id=2;
		e2.name="Appu";
		e2.salary=30000;
		e2.age=26;
		
		e2.id=3;
		e2.name="Rutu";
		e2.salary=3500;
		e2.age=19;
		
	
	}
	
}


