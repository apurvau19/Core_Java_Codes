package basicPrograms;

public class IdentifierRulesEx1{

	public static void main(String[] args){
	//int ^%#=10;	shows CE
		//Rule1: identifiers should start with letter only
		int total= 10;
		//int num# = 20;
		int abc= 19;
		//int 1fish = 30;
		int fish = 30;
		
		//Rule2: identifiers are case-sensitive
		int num = 2;
		int Num = 8;
		int NUM = 5;
		
		//
		//int if = 29; CE
		
		
		
		
		int[] arr= new int[5];
		
		arr[8]=10;
		
		System.out.println("Program ends here.");
		
		
		
		
		
		
	}

}
