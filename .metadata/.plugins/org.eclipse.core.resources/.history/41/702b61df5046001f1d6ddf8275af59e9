package basicPrograms;

import java.util.ArrayList;



public class HelloWorld {
public static void main(String[] args) {
	
	ArrayList al= new ArrayList();
	al.add(5);
	al.add(8);
	al.add(4);
	al.add(6);
	al.add("sourabh");
	al.add(23.56);
	al.add(true);
	
	al.sort(null);
	
	System.out.println(al);
}
	







}
